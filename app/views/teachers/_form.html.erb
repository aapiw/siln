<%= form_with(model: teacher, local: true) do |form| %>
  <% if teacher.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(teacher.errors.count, "error") %> prohibited this teacher from being saved:</h2>

      <ul>
      <% teacher.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :school_id %>
    <%= form.text_field :school_id, id: :teacher_school_id %>
  </div>

  <div class="field">
    <%= form.label :sk_submission_id %>
    <%= form.text_field :sk_submission_id, id: :teacher_sk_submission_id %>
  </div>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :teacher_name %>
  </div>

  <div class="field">
    <%= form.label :pns %>
    <%= form.check_box :pns, id: :teacher_pns %>
  </div>

  <div class="field">
    <%= form.label :age %>
    <%= form.text_field :age, id: :teacher_age %>
  </div>

  <div class="field">
    <%= form.label :period_of_teaching %>
    <%= form.text_field :period_of_teaching, id: :teacher_period_of_teaching %>
  </div>

  <div class="field">
    <%= form.label :number_of_extension %>
    <%= form.number_field :number_of_extension, id: :teacher_number_of_extension %>
  </div>

  <div class="field">
    <%= form.label :expire %>
    <%= form.date_select :expire, id: :teacher_expire %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
