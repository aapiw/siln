<%# if action_name ==  'index' and controller_name == 'landings'
		klass  = 'container' 
	else
		klass = 'container-flash'
	end 
%>

<%if flash.present? %>
<!-- <div class=" "> -->
		<% flash.each do |key, value| %>
		<% color = {alert:"danger", notice:"success" } %>
		<div class="sufee-alert alert with-close alert-<%=color[key.to_sym] %> alert-dismissible fade show">
		  <% if value.respond_to? :each %>
			  <ul>
				  <% value.each do |message| %>
				  	<li><%= message %></li>
				  <% end %>
			  </ul>
		  <% else %>
			  	<%= value %>
			 <% end %>
	    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
	      <span aria-hidden="true">Ã—</span>
	  </button>
	</div>
<% end %>
<% end %>
